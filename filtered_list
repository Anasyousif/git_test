valid_formats = [
    "docx",
    "pdf",
    "txt",
    "pptx",
    "ppt",
    "md",
]

# Don't edit above this line


def pair_document_with_format(doc_names, doc_formats):
    new_list = list(zip(doc_names, doc_formats))
    
    # 2. Filter the list
    # We check if the format (the second element, x[1]) is in valid_formats
    filtered_items = filter(lambda x: x[1] in valid_formats, new_list)
    
    # 3. Return as a list
    return list(filtered_items)